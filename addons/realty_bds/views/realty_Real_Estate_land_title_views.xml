<odoo>
	<!-- Admin Tree View -->
	<record id="land_title_tree" model="ir.ui.view">
		<field name="name">Land Title Tree</field>
		<field name="model">land_title</field>
		<field name="arch" type="xml">
			<list>
				<field name="name"/>
			</list>
		</field>
	</record>

	<!-- Admin Form View with Auto-Suggestion -->
	<record id="land_tittle_form" model="ir.ui.view">
		<field name="name">Land Title Form</field>
		<field name="model">land_title</field>
		<field name="arch" type="xml">
			<form>
				<sheet>
					<group>
						<field id="land_title_name" name="name" placeholder="Enter a name" autocomplete="off"/>
					</group>
				</sheet>
				<chatter/>
			</form>
		</field>
	</record>

	<!-- Admin Menu -->
	<record id="action_land_title" model="ir.actions.act_window">
		<field name="name">Land Title Action</field>
		<field name="res_model">land_title</field>
		<field name="view_mode">list,form</field>
	</record>
</odoo>